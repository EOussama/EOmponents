{
  "name": "eomponents",
  "version": "1.0.0",
  "description": "A flat and responsive CSS stylesheet.",
  "main": "src/less/main.less",
  "scripts": {
    "build": "node ./node_modules/less/bin/lessc ./src/less/eomponents.less ./dist/css/eomponents.css",
    "build:w": "node ./node_modules/less-watch-compiler/dist/less-watch-compiler.js ./src/less ./dist/css eomponents.less",
    "js": "cp -r ./src/js ./dist/js",
    "prod": "npm run build && npm run js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EOussama/EOmponents.git"
  },
  "keywords": [
    "EOmponents",
    "CSS",
    "flat",
    "framework",
    "responsive"
  ],
  "author": "Oussama Essamadi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/EOussama/EOmponents/issues"
  },
  "homepage": "https://github.com/EOussama/EOmponents#readme",
  "devDependencies": {
    "less": "^3.8.1",
    "less-watch-compiler": "^1.13.0"
  }
}
